{
  "version": 2,
  "rewrites": [
    { 
      "source": "/search", 
      "destination": "https://your-ibm-link.appdomain.cloud/search" 
    },
    { 
      "source": "/results", 
      "destination": "https://your-ibm-link.appdomain.cloud/results" 
    },
    { 
      "source": "/(.*)", 
      "destination": "https://your-ibm-link.appdomain.cloud/$1" 
    }
  ]
}